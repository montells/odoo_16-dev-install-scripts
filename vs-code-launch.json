    "version": "0.2.0",
    "configurations": [
    {
      "name": "Odoo",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/odoo/odoo-bin",
      "args": ["-c", "${workspaceFolder}/odoo.conf", "-i", "base"],
      "gevent": false,
      "envFile": "${workspaceFolder}/debug.env",
      "console": "integratedTerminal",
      "justMyCode": false,
      "python": "${workspaceRoot}/.venv/bin/python3"
    }
  ]
}
